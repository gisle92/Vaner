--Commands to run before 

cd /workspaces/Vaner
ls

export PATH="$PATH:/workspaces/Vaner/flutter/bin:$HOME/.pub-cache/bin"
flutter --version

cd /workspaces/Vaner/vaner
flutter pub get

flutter run -d web-server --web-hostname 0.0.0.0 --web-port 8080


--release: 

cd /workspaces/Vaner/vaner

export PATH="$PATH:/workspaces/Vaner/flutter/bin:$HOME/.pub-cache/bin"
flutter --version

flutter clean
flutter pub get
flutter build appbundle --release
